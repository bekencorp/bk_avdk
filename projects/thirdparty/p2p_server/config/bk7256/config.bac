#
# Feature fast connect
#
CONFIG_WIFI_FAST_CONNECT=n
CONFIG_WIFI_FAST_DHCP=n
CONFIG_IPV6=n
CONFIG_SAVE_BOOT_TIME_POINT=n

#
# Compile/Build
#

#
# System
#
CONFIG_CPU_CNT=1
CONFIG_MAILBOX=n
# CONFIG_SYS_CPU0=y
## CONFIG_SYS_CPU1_OFFSET 65536 == 0x10000 in bk7256_cp1_bsp.ld
# CONFIG_SYS_CPU1_OFFSET=0x10000
#


CONFIG_CUSTOMIZE_HEAP_SIZE=237568

#
# Relative path for armino
#
#CONFIG_TOOLCHAIN_PATH="../../toolchain/opt/risc-v/nds32le-elf-mculib-v5/bin"


CONFIG_CS2_P2P_SERVER=y
CONFIG_CS2_P2P_CLIENT=n

CONFIG_DOORBELL=y

CONFIG_TRANSFER_TASK_STACK_SIZE=2048
CONFIG_MEDIA_MAJOR_THREAD_STACK_SIZE=2048

CONFIG_OVERRIDE_FLASH_PARTITION=y